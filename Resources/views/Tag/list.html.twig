{% extends 'ASFBlogBundle::backend_layout.html.twig' %}

{% trans_default_domain 'asf_blog' %}

{% block title %}{{ 'Tags list'|trans }}{% endblock %}

{% block asf_blog_content %}

	{% block page_title %}{{ 'Tags list'|trans }}{% endblock %}
	
	{% block breadcrumbs_tree %}
	 > <a href="{{ path('asf_blog_tag_list') }}" title="{{ 'Tags list'|trans }}">{{ 'Tags list'|trans }}</a>
	{% endblock %}
	
	{% block asf_content_cmd %}
	<a class="btn btn-default" href="{{ path('asf_blog_tag_add') }}" title="{{ 'Add new tag'|trans }}">{{ 'Add new tag'|trans }}</a>
	{% endblock asf_content_cmd %}
	
	<div class="row">
		<div class="col-xs-12">
			{{ grid(grid) }}
		</div>
	</div>
	
{% endblock %}